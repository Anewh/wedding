      </div>

      <div class="material-color-picker w-8/12 md:w-6/12">
        <div class="material-color-picker__left-panel">
          <ol class="color-selector" data-bind="foreach: materialColors">
            <li id="main-color-pick-element">
              <input name="material-color" type="radio"
                     data-bind="attr: { id: 'materialColor' + $index() }, checked: $parent.selectedColor, value: name">
              <label
                     data-bind="attr: { for: 'materialColor' + $index(), title: $data.name }, style: { 'color': color }"></label>
            </li>
          </ol>
        </div>
        <div class="material-color-picker__right-panel" data-bind="foreach: materialColors">
          <div class="color-palette-wrapper" data-bind="css: { 'js-active': $parent.selectedColor() === name }">
            <div class="color-palette__item"
                 data-bind="style: { 'background-color': color }">
              <span data-bind="text: name"></span>
              {{!-- <span data-bind="text: color"></span> --}}
            </div>
            <ol class="color-palette" data-bind="foreach: variations">
              <li class="color-palette__item"
                  data-bind="style: { 'background-color': color }">
                <span data-bind="text: name"></span>
                {{!-- <span data-bind="text: color"></span> --}}
              </li>
            </ol>
          </div>
        </div>
      </div>

